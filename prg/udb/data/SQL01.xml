<!-- description of dictonary SQL01 - sample dictionary -->

<depository    	name="depository1" 		memosize="&(codb_info('CODB_MEMOSIZE_DEFAULT')")>

<extent    	name="statedep"	help="Information from state departament of statistic">

<attribute 	name="code" 			type="C" len=10  lentype=3>
<attribute 	name="name"    			type="C" len=20  lentype=3>
<attribute 	name="unit" 			type="C" len=7   lentype=2>
<attribute 	name="smallname"		type="C" len=50  lentype=2>

<index     	name="code"    			expr="code">
<index     	name="name"    			expr="name">
<index     	name="unit"    			expr="unit">
<index     	name="smallname"		expr="smallname">

<class		name="currency"		super_id="" extent_id="statedep"
		expr_essence="name"
		attr_list="&({'code','unit','name','smallname'})"
		 idx_list="&({'code','unit','name','smallname'})"
	>

<tcolumn	name="code"		width="10"
		header="Код"		footer=""
		expr="code"
	>

<tcolumn	name="unit"		width="7"
		header="ед.изм"		footer=""
		expr="unit"
	>

<tcolumn	name="name"		width="30"
		header="Наименование"    footer=""
		expr="name"
	>

<tcolumn	name="smallname"	width="30"
		header="короткое имя"    footer=""
		expr="smallname"
	>

<tcolumn	name="currency"  	width="12"
		header="Валюта"		footer=""
		expr="currency"
	>

<tview		name="Валюта"
		class_id="currency"
		col_list="&({'code','unit','name','smallname'})"
	>

